{
  "name": "less-framework-root",
  "version": "0.0.0",
  "description": "less framework base on less-api",
  "private": "true",
  "scripts": {
    "install": "lerna bootstrap",
    "start": "lerna run start",
    "build": "lerna run build",
    "watch": "lerna run watch",
    "init": "lerna run init",
    "prepublishOnly": "npm run build && npm run copy-readme",
    "init-start": "npm run init && npm run start",
    "copy-readme": "npx copy-cli ./packages/less-api-framework/README.md ./README.md"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api"
  ],
  "devDependencies": {
    "copy": "^0.3.2",
    "husky": "^5.2.0"
  }
}
