{
  "name": "less-framework",
  "version": "0.0.1",
  "description": "less framework base on less-api",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p tsconfig.json",
    "watch": "npx tsc -p tsconfig.json -w",
    "init": "node init/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "less-api": "^1.3.5",
    "log4js": "^6.3.0",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "uuid": "^8.3.2",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/mongodb": "^3.6.10",
    "@types/multer": "^1.4.5",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3"
  }
}
