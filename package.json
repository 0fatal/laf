{
  "name": "less-framework",
  "version": "0.0.1",
  "description": "less framework base on less-api",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p tsconfig.json && cp -R ./src/rules/ ./dist/rules/",
    "watch": "cp -R ./src/rules/ ./dist/rules/ && npx tsc -p tsconfig.json -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api"
  ],
  "dependencies": {
    "express": "^4.17.1",
    "less-api": "^1.2.8",
    "mongodb": "^3.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11"
  }
}