{
  "name": "less-framework",
  "version": "0.1.4",
  "description": "less framework base on less-api",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node --experimental-vm-modules ./dist/index.js",
    "build": "npx tsc -p tsconfig.json",
    "watch": "npx tsc -p tsconfig.json -w",
    "init": "node init/index.js",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "commit": "commit",
    "init-start": "npm run build && npm run init && npm run start"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "less-api": "^1.4.4",
    "less-api-database": "^0.3.7",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "uuid": "^8.3.2",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@commitlint/prompt-cli": "^12.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongodb": "^3.6.10",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.37",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "husky": "^5.2.0",
    "typescript": "^4.2.3"
  }
}
