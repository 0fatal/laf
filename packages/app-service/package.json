{
  "name": "app-service",
  "version": "0.6.8",
  "description": "the app engine service of lat",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": "true",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p tsconfig.json && npm run create-internal-pkg",
    "watch": "npx tsc -p tsconfig.json -w",
    "create-internal-pkg": "node ./scripts/create-internal-package.js",
    "prepublishOnly": "npm run build",
    "trace-gc": "node --trace_gc --trace_gc_verbose ./dist/index.js",
    "init-start": "npm run start"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "cloud-function-engine": "^0.6.8",
    "database-proxy": "^0.6.4",
    "dayjs": "^1.10.7",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mongodb": "^4.1.3",
    "mongodb-uri": "^0.9.7",
    "multer": "^1.4.2",
    "node-modules-utils": "^0.6.2",
    "nodemailer": "^6.6.3",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.7",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.171",
    "@types/mongodb-uri": "^0.9.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^16.7.10",
    "@types/nodemailer": "^6.4.4",
    "@types/validator": "^13.1.3",
    "typescript": "^4.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*",
      "http/*"
    ],
    "delay": 1000
  }
}
