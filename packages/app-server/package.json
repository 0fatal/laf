{
  "name": "laf-app-server",
  "version": "0.4.19",
  "description": "less framework base on less-api",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": "true",
  "scripts": {
    "start": "node ./dist/start.js",
    "build": "npx tsc -p tsconfig.json && npm run create-internal-pkg",
    "watch": "npx tsc -p tsconfig.json -w",
    "create-internal-pkg": "node ./scripts/create-internal-package.js",
    "prepublishOnly": "npm run build",
    "trace-gc": "node --trace_gc --trace_gc_verbose ./dist/index.js",
    "init-start": "npm run start"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api"
  ],
  "dependencies": {
    "ali-oss": "^6.16.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cloud-function-engine": "^0.4.3",
    "dayjs": "^1.10.6",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "less-api": "^1.5.3",
    "less-api-database": "^1.5.3",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mongodb": "^4.0.1",
    "multer": "^1.4.2",
    "node-modules-utils": "^0.4.17",
    "nodemailer": "^6.6.3",
    "uuid": "^8.3.2",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.10",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.7",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.171",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.37",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "typescript": "^4.2.3"
  }
}
