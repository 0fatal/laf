{
  "name": "laf-devops-server",
  "version": "0.0.1",
  "description": "less api framework - devops server",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": "true",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p tsconfig.json",
    "watch": "npx tsc -p tsconfig.json -w",
    "init": "node init/index.js",
    "prepublishOnly": "npm run build",
    "init-start": "npm run init && npm run start"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api",
    "devops"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "less-api": "^1.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mongodb": "^4.0.1",
    "uuid": "^8.3.2",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.171",
    "@types/mongodb": "^3.6.10",
    "@types/node": "^14.14.37",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "typescript": "^4.2.3"
  }
}
