{
  "name": "laf-devops-server",
  "version": "0.4.21-alpha.0",
  "description": "less api framework - devops server",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": "true",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p tsconfig.json",
    "watch": "npx tsc -p tsconfig.json -w",
    "init": "node init/index.js",
    "prepublishOnly": "npm run build",
    "init-start": "npm run init && npm run start"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api",
    "devops"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "cloud-function-engine": "^0.4.3",
    "dayjs": "^1.10.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "less-api": "^1.5.4",
    "log4js": "^6.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "@types/uuid": "^8.3.0",
    "typescript": "^4.2.3"
  }
}
