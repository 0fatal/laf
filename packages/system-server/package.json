{
  "name": "system-server",
  "version": "0.5.0-alpha.2",
  "description": "less api framework - system server",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": "true",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p tsconfig.json",
    "watch": "npx tsc -p tsconfig.json -w",
    "init": "node init/index.js",
    "prepublishOnly": "npm run build",
    "init-start": "npm run init && npm run start"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "mysql",
    "acl",
    "serverless",
    "less-api",
    "devops"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "cloud-function-engine": "^0.4.3",
    "dayjs": "^1.10.6",
    "dockerode": "^3.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "less-api": "^1.5.4",
    "log4js": "^6.3.0",
    "mongodb": "^4.1.1",
    "mongodb-uri": "^0.9.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/dockerode": "^3.2.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongodb-uri": "^0.9.1",
    "@types/node": "^16.7.10",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.12",
    "typescript": "^4.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*",
      "http/*"
    ],
    "delay": 1000
  }
}
